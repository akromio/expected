{
  "name": "@akromio/expected-helpers",
  "version": "0.1.0",
  "description": "Helpers for the assert libs.",
  "homepage": "https://akromio.com",
  "author": {
    "name": "Sia Codelabs",
    "email": "hello@siacodelabs.com",
    "url": "http://siacodelabs.com"
  },
  "contributors": [
    {
      "name": "Raúl G. González",
      "email": "raulggonzalez@siacodelabs.com"
    }
  ],
  "keywords": [
    "assert",
    "expect",
    "test"
  ],
  "bugs": {
    "url": "https://github.com/akromio/expected/issues",
    "email": "bugs@akromio.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/akromio/expected.git"
  },
  "preferGlobal": false,
  "type": "commonjs",
  "main": "dist/cjs/index.js",
  "files": [
    "dist/"
  ],
  "engines": {
    "node": ">= 14",
    "npm": ">= 6"
  },
  "dependencies": {
    "@dogmalang/core": "^1.0.0-rc18.0",
    "chalk": "^4.1.2"
  },
  "scripts": {
    "lint": "dogmac check src tests",
    "build": "npm run build/src",
    "build/src": "rm -rf dist/ && dogmac js -o dist/cjs/ src && babel -d dist/ dist/",
    "test": "npm run test/build && mocha --config .mocharc.yaml",
    "test/build": "rm -rf ./__tests__ && dogmac js -o __tests__ tests && babel -d __tests__ __tests__",
    "cov": "c8 mocha --config .mocharc.yaml"
  }
}
